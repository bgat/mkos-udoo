[general]
arch=armhf
noauth=true
unpack=true
cleanup=true
omitrequired=true
markauto=true
bootstrap=ptux emdebian debian
aptsources=
aptpreferences=/usr/share/mkos-udoo/preferences

[ptux]
source=http://packages.pragmatux.org/device
suite=3.0
components=main
packages={{device-package}}

[emdebian]
source=http://emdebian.org/grip
suite=wheezy-grip

[debian]
source=http://cdn.debian.net/debian
suite=wheezy
